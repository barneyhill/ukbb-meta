{
  "name": "split_plinks_by_anc",
  "title": "Split Plinks",
  "summary": "split_plinks_by_anc",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "inputSpec": [
    {
      "name": "QCd_pop_IDs",
      "label": "Sample IDs of QCd exome data",
      "class": "file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "superpopulation",
      "label": "Superpopulation sample IDs",
      "class": "file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "plink_bed",
      "class": "file",
      "optional": true,
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "plink_bim",
      "class": "file",
      "optional": true,
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "plink_fam",
      "class": "file",
      "optional": true,
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "plink_files",
      "class": "array:file",
       "patterns": ["*"]
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 48
      }
    },
    "interpreter": "bash",
    "file": "src/split_plinks_by_anc.sh",
    "distribution": "Ubuntu",
    "release": "20.04",
    "version": "0"
  },
  "regionalOptions": {
    "aws:eu-west-2": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_v2_x2"
        }
      }
    }
  }
}
